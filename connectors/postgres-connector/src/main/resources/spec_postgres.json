{
  "properties": {
    "name": "PostgreSQL",
    "icon": "icons/icon.svg",
    "id": "postgres"
  },
  "configOptions": {
    "connection": {
      "type": "object",
      "properties": {
        "host": {
          "type": "string",
          "title": "Host",
          "x-decorator": "FormItem",
          "x-component": "Input"
        },
        "port": {
          "type": "string",
          "title": "Port",
          "x-decorator": "FormItem",
          "x-component": "Input"
        },
        "database": {
          "type": "string",
          "title": "Database",
          "x-decorator": "FormItem",
          "x-component": "Input"
        },
        "schema": {
          "type": "string",
          "title": "Schema",
          "x-decorator": "FormItem",
          "x-component": "Input"
        },
        "extParams": {
          "type": "string",
          "title": "ExtendParams",
          "x-decorator": "FormItem",
          "x-component": "Input"
        },
        "user": {
          "type": "string",
          "title": "User",
          "x-decorator": "FormItem",
          "x-component": "Input"
        },
        "password": {
          "type": "string",
          "title": "Password",
          "x-decorator": "FormItem",
          "x-component": "Password"
        }
      }
    }
  },
  "dataTypes": {
    "smallint": {
      "bit": 16,
      "priority": 3,
      "to": "TapNumber"
    },
    "integer": {
      "bit": 32,
      "priority": 1,
      "to": "TapNumber"
    },
    "bigint": {
      "bit": 64,
      "priority": 3,
      "to": "TapNumber"
    },
    "numeric[($precision,$scale)]": {
      "bit": 128,
      "precision": [
        1,
        1000
      ],
      "scale": [
        0,
        1000
      ],
      "priority": 1,
      "to": "TapNumber"
    },
    "real": {
      "bit": 32,
      "priority": 3,
      "queryOnly": true,
      "to": "TapNumber"
    },
    "double precision": {
      "bit": 64,
      "priority": 3,
      "queryOnly": true,
      "to": "TapNumber"
    },
    "char[($byte)]": {
      "byte": 10485760,
      "priority": 2,
      "to": "TapString"
    },
    "varchar[($byte)]": {
      "byte": 10485760,
      "priority": 1,
      "to": "TapString"
    },
    "text": {
      "byte": "4g",
      "priority": 2,
      "to": "TapString"
    },
    "bytea": {
      "byte": "4g",
      "priority": 1,
      "to": "TapBinary"
    },
    "bit[($byte)]": {
      "byte": 64,
      "defaultByte": 1,
      "priority": 3,
      "queryOnly": true,
      "to": "TapBinary"
    },
    "bit varying": {
      "byte": 64,
      "priority": 3,
      "queryOnly": true,
      "to": "TapBinary"
    },
    "boolean": {
      "bit": 1,
      "priority": 1,
      "to": "TapBoolean"
    },
    "date": {
      "range": [
        "0001-01-01",
        "9999-12-31"
      ],
      "priority": 1,
      "to": "TapDate"
    },
    "interval($rangeType)": {
      "range": [
        "-99:59:59",
        "99:59:59"
      ],
      "priority": 3,
      "queryOnly": true,
      "to": "TapTime"
    },
    "timestamp[($fsp)]": {
      "range": [
        "1000-01-01 00:00:00",
        "9999-12-31 23:59:59"
      ],
      "fsp": [0,6],
      "defualtFsp":19,
      "priority": 3,
      "to": "TapDateTime"
    }
  }
}

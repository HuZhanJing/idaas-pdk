{
  "properties": {
    "name": "GBase-8a",
    "icon": "icons/icon.png",
    "id": "gbase8a"
  },
  "configOptions": {
    "connection": {
      "type": "object",
      "properties": {
        "host": {
          "type": "string",
          "title": "Host",
          "x-decorator": "FormItem",
          "x-component": "Input"
        },
        "port": {
          "type": "string",
          "title": "Port",
          "x-decorator": "FormItem",
          "x-component": "Input"
        },
        "database": {
          "type": "string",
          "title": "Database",
          "x-decorator": "FormItem",
          "x-component": "Input"
        },
        "extParams": {
          "type": "string",
          "title": "ExtendParams",
          "x-decorator": "FormItem",
          "x-component": "Input"
        },
        "user": {
          "type": "string",
          "title": "User",
          "x-decorator": "FormItem",
          "x-component": "Input"
        },
        "password": {
          "type": "string",
          "title": "Password",
          "x-decorator": "FormItem",
          "x-component": "Input"
        }
      }
    }
  },
  "dataTypes": {
    "tinyint [unsigned] [zerofill]": {
      "bit": 8,
      "priority": 3,
      "to": "TapNumber"
    },
    "smallint [unsigned] [zerofill]": {
      "bit": 16,
      "priority": 3,
      "to": "TapNumber"
    },
    "mediumint [unsigned] [zerofill]": {
      "bit": 24,
      "priority": 3,
      "to": "TapNumber"
    },
    "int[eger] [unsigned] [zerofill]": {
      "bit": 32,
      "priority": 1,
      "to": "TapNumber"
    },
    "bigint [unsigned] [zerofill]": {
      "bit": 64,
      "priority": 3,
      "to": "TapNumber"
    },
    "float[($precision,$scale)] [unsigned] [zerofill]": {
      "bit": 32,
      "precision": [1,255],
      "defaultPrecision": 10,
      "scale": [0,30],
      "defaultPrecision": 2,
      "priority": 3,
      "to": "TapNumber"
    },
    "double[($precision,$scale)] [unsigned] [zerofill]": {
      "bit": 64,
      "precision": [1,255],
      "defaultPrecision": 16,
      "scale": [0,30],
      "defaultPrecision": 4,
      "priority": 3,
      "to": "TapNumber"
    },
    "decimal[($precision,$scale)] [unsigned] [zerofill]": {
      "bit": 128,
      "precision": [0,65],
      "defaultPrecision": 10,
      "scale": [0,30],
      "defaultScale": 0,
      "priority": 1,
      "to": "TapNumber"
    },
    "date": {
      "range": [
        "0000-01-01",
        "9999-12-31"
      ],
      "priority": 1,
      "to": "TapDate"
    },
    "time": {
      "range": [
        "-838:59:59",
        "838:59:59"
      ],
      "priority": 3,
      "to": "TapTime"
    },
    "year": {
      "range": [
        "1901",
        "2155"
      ],
      "priority": 1,
      "to": "TapYear"
    },
    "datetime[($fsp)]": {
      "range": [
        "1000-01-01 00:00:00",
        "9999-12-31 23:59:59"
      ],
      "fsp": [0,6],
      "defualtFsp":0,
      "priority": 1,
      "to": "TapDateTime"
    },
    "timestamp[($fsp)]": {
      "range": [
        "1970-01-01 00:00:00",
        "2038-01-19 11:14:07"
      ],
      "fsp": [0,6],
      "defualtFsp":0,
      "priority": 3,
      "to": "TapDateTime"
    },
    "char[($byte)]": {
      "byte": 255,
      "defaultByte": 1,
      "bitRatio": 3,
      "priority": 2,
      "to": "TapString"
    },
    "varchar($byte)": {
      "byte": "64k",
      "bitRatio": 3,
      "priority": 1,
      "to": "TapString"
    },
    "tinyblob": {
      "byte": 255,
      "priority": 3,
      "to": "TapBinary"
    },
    "tinytext": {
      "byte": 255,
      "priority": 3,
      "to": "TapString"
    },
    "blob": {
      "byte": "64k",
      "priority": 2,
      "to": "TapBinary"
    },
    "text": {
      "byte": "64k",
      "priority": 2,
      "to": "TapString"
    },
    "mediumblob": {
      "byte": "16m",
      "priority": 3,
      "to": "TapBinary"
    },
    "mediumtext": {
      "byte": "16m",
      "priority": 3,
      "to": "TapString"
    },
    "longblob": {
      "byte": "4g",
      "priority": 2,
      "to": "TapBinary"
    },
    "longtext": {
      "byte": "4g",
      "priority": 2,
      "to": "TapString"
    },
    "bit[($byte)]": {
      "byte": 64,
      "defaultByte": 1,
      "priority": 3,
      "to": "TapBinary"
    },
    "binary([$byte])": {
      "byte": 255,
      "defaultByte":1,
      "priority": 1,
      "to": "TapBinary"
    },
    "json": {
      "byte": "64k",
      "priority": 3,
      "to": "TapString"
    }
  }
}

{
  "properties": {
	"name": "Mysql",
	"icon": "icons/mysql.png",
	"id": "mysql"
  },
  "configOptions": {
	"connection": {
	  "type": "object",
	  "properties": {
		"host": {
		  "type": "string",
		  "title": "Host",
		  "x-decorator": "FormItem",
		  "x-component": "Input"
		},
		"port": {
		  "type": "string",
		  "title": "Port",
		  "x-decorator": "FormItem",
		  "x-component": "InputNumber",
		  "default": 3306
		},
		"database": {
		  "type": "string",
		  "title": "Database Name",
		  "x-decorator": "FormItem",
		  "x-component": "Input"
		},
		"username": {
		  "type": "string",
		  "title": "Username",
		  "x-decorator": "FormItem",
		  "x-component": "Password"
		},
		"password": {
		  "type": "string",
		  "title": "Password",
		  "x-decorator": "FormItem",
		  "x-component": "Input"
		},
		"addtionalString": {
		  "type": "string",
		  "title": "Connection Parameter String",
		  "x-decorator": "FormItem",
		  "x-component": "Input"
		},
		"isShareCdc": {
		  "type": "boolean",
		  "title": "Open Share CDC",
		  "x-visible": false,
		  "x-reactions": "{{isShareCdc}}"
		},
		"timezone": {
		  "type": "string",
		  "title": "Timezone",
		  "default": "Database Timezone",
		  "enum": [
			{
			  "label": "Database Timezone",
			  "value": ""
			},
			{
			  "label": "UTC -11",
			  "value": "-11:00"
			},
			{
			  "label": "UTC -10",
			  "value": "-10:00"
			},
			{
			  "label": "UTC -09",
			  "value": "-09:00"
			},
			{
			  "label": "UTC -08",
			  "value": "-08:00"
			},
			{
			  "label": "UTC -07",
			  "value": "-07:00"
			},
			{
			  "label": "UTC -06",
			  "value": "-06:00"
			},
			{
			  "label": "UTC -05",
			  "value": "-05:00"
			},
			{
			  "label": "UTC -04",
			  "value": "-04:00"
			},
			{
			  "label": "UTC -03",
			  "value": "-03:00"
			},
			{
			  "label": "UTC -02",
			  "value": "-02:00"
			},
			{
			  "label": "UTC -01",
			  "value": "-01:00"
			},
			{
			  "label": "UTC",
			  "value": "+00:00"
			},
			{
			  "label": "UTC +01",
			  "value": "+01:00"
			},
			{
			  "label": "UTC +02",
			  "value": "+02:00"
			},
			{
			  "label": "UTC +03",
			  "value": "+03:00"
			},
			{
			  "label": "UTC +04",
			  "value": "+04:00"
			},
			{
			  "label": "UTC +05",
			  "value": "+05:00"
			},
			{
			  "label": "UTC +06",
			  "value": "+06:00"
			},
			{
			  "label": "UTC +07",
			  "value": "+07:00"
			},
			{
			  "label": "UTC +08",
			  "value": "+08:00"
			},
			{
			  "label": "UTC +09",
			  "value": "+09:00"
			},
			{
			  "label": "UTC +10",
			  "value": "+10:00"
			},
			{
			  "label": "UTC +11",
			  "value": "+11:00"
			},
			{
			  "label": "UTC +12",
			  "value": "+12:00"
			},
			{
			  "label": "UTC +13",
			  "value": "+13:00"
			},
			{
			  "label": "UTC +14",
			  "value": "+14:00"
			}
		  ]
		}
	  }
	}
  },
  "dataTypes": {
	"char[($byte)]": {
	  "to": "TapString",
	  "byte": 255,
	  "defaultByte": 1,
	  "fixed": true
	},
	"varchar($byte)": {
	  "to": "TapString",
	  "byte": 65535,
	  "defaultByte": 1
	},
	"tinytext": {
	  "to": "TapString",
	  "byte": 255
	},
	"text": {
	  "to": "TapString",
	  "byte": "64k"
	},
	"mediumtext": {
	  "to": "TapString",
	  "byte": "16m"
	},
	"longtext": {
	  "to": "TapString",
	  "byte": "4g"
	},
	"binary[($byte)]": {
	  "to": "TapBinary",
	  "byte": 255,
	  "defaultByte": 1,
	  "fixed": true
	},
	"varbinary[($byte)]": {
	  "to": "TapBinary",
	  "byte": 65535,
	  "defaultByte": 1
	},
	"tinyblob": {
	  "to": "TapBinary",
	  "byte": 255
	},
	"blob": {
	  "to": "TapBinary",
	  "byte": "64k"
	},
	"mediumblob": {
	  "to": "TapBinary",
	  "byte": "16m"
	},
	"longblob": {
	  "to": "TapBinary",
	  "byte": "4g"
	},
	"bit[($bit)]": {
	  "to": "TapNumber",
	  "bit": 64,
	  "precision": 20,
	  "value": [
		0,
		18446744073709552000
	  ]
	},
	"tinyint[($m)]": {
	  "to": "TapNumber",
	  "bit": 8,
	  "precision": 3,
	  "value": [
		0,
		255
	  ]
	},
	"tinyint[($m)] unsigned": {
	  "to": "TapNumber",
	  "bit": 8,
	  "precision": 3,
	  "value": [
		-128,
		127
	  ],
	  "unsigned": "unsigned"
	},
	"smallint[($m)]": {
	  "to": "TapNumber",
	  "bit": 16,
	  "value": [
		-32768,
		32767
	  ],
	  "precision": 5
	},
	"smallint[($m)] unsigned": {
	  "to": "TapNumber",
	  "bit": 16,
	  "precision": 5,
	  "value": [
		0,
		65535
	  ],
	  "unsigned": "unsigned"
	},
	"mediumint[($m)]": {
	  "to": "TapNumber",
	  "bit": 32,
	  "precision": 8,
	  "value": [
		-8388608,
		8388607
	  ]
	},
	"mediumint[($m)] unsigned": {
	  "to": "TapNumber",
	  "bit": 32,
	  "precision": 8,
	  "value": [
		0,
		16777215
	  ],
	  "unsigned": "unsigned"
	},
	"int[($m)]": {
	  "to": "TapNumber",
	  "bit": 16,
	  "precision": 10,
	  "value": [
		-2147483648,
		2147483647
	  ]
	},
	"int[($m)] unsigned": {
	  "to": "TapNumber",
	  "bit": 32,
	  "precision": 10,
	  "value": [
		0,
		4294967295
	  ]
	},
	"bigint[($m)]": {
	  "to": "TapNumber",
	  "bit": 64,
	  "precision": 19,
	  "value": [
		-9223372036854775808,
		9223372036854775807
	  ]
	},
	"bigint[($m)] unsigned": {
	  "to": "TapNumber",
	  "bit": 64,
	  "precision": 20,
	  "value": [
		0,
		18446744073709551615
	  ]
	},
	"decimal[($precision, $scale)][unsigned]": {
	  "to": "TapNumber",
	  "precision": [
		1,
		65
	  ],
	  "scale": [
		0,
		30
	  ],
	  "defaultPrecision": 10,
	  "defaultScale": 0,
	  "unsigned": "unsigned"
	},
	"float[($precision, $scale)][unsigned]": {
	  "to": "TapNumber",
	  "precision": [
		1,
		30
	  ],
	  "scale": [
		0,
		30
	  ],
	  "value": [
		"-3.402823466E+38",
		"3.402823466E+38"
	  ],
	  "unsigned": "unsigned",
	  "fixed": false
	},
	"float": {
	  "to": "TapNumber",
	  "precision": [
		1,
		6
	  ],
	  "scale": [
		0,
		6
	  ],
	  "fixed": false
	},
	"double": {
	  "to": "TapNumber",
	  "precision": [
		1,
		11
	  ],
	  "scale": [
		0,
		11
	  ],
	  "fixed": false
	},
	"double[($precision, $scale)][unsigned]": {
	  "to": "TapNumber",
	  "precision": [
		1,
		255
	  ],
	  "scale": [
		0,
		30
	  ],
	  "value": [
		"-1.7976931348623157E+308",
		"1.7976931348623157E+308"
	  ],
	  "unsigned": "unsigned",
	  "fixed": false
	},
	"date": {
	  "to": "TapDate",
	  "range": [
		"1000-01-01",
		"9999-12-31"
	  ],
	  "format": "yyyy-MM-dd"
	},
	"time": {
	  "to": "TapTime",
	  "range": [
		"-838:59:59",
		"838:59:59"
	  ]
	},
	"datetime[($precision)]": {
	  "to": "TapDateTime",
	  "range": [
		"1000-01-01 00:00:00.000000",
		"9999-12-31 23:59:59.999999"
	  ],
	  "format": "yyyy-MM-dd HH:mm:ss.SSSSSS",
	  "precision": [
		0,
		6
	  ],
	  "defaultPrecision": 0
	},
	"timestamp[($precision)]": {
	  "to": "TapDateTime",
	  "range": [
		"1970-01-01 00:00:01.000000",
		"2038-01-19 03:14:07.999999"
	  ],
	  "format": "yyyy-MM-dd HH:mm:ss.SSSSSS",
	  "precision": [
		0,
		6
	  ],
	  "defaultPrecision": 0
	}
  }
}